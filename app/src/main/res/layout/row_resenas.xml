<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="resenasRowModel" type="com.example.myhome.modules.resenas.data.model.ResenasRowModel"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearValoracionar"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700_4">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowellipseeleven"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_5pxh"
            android:layout_marginTop="@dimen/_6pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageEllipseEleven"
                android:layout_height="@dimen/_34pxh"
                android:layout_width="@dimen/_34pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_ellipse11}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse11"/>
            <TextView android:ellipsize="end" android:id="@+id/txtAriel"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.63" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{resenasRowModel.txtAriel}"
                style="@style/txtOpenSansopensansbold24" tools:text="@string/lbl_ariel"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowratingbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_5pxh"
            android:layout_marginTop="@dimen/_2pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <androidx.appcompat.widget.AppCompatRatingBar
                android:id="@+id/ratingBarRatingbar"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_weight="0.91" android:layout_width="0dp"
                android:numStars="5"
                android:progressBackgroundTint="@color/gray_500"
                android:progressTint="@color/amber_500"
                android:rating="4" style="@style/Widget.AppCompat.RatingBar"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFortyFive"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_weight="0.09" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{resenasRowModel.txtFortyFive}"
                style="@style/txtOpenSansopensansbold12" tools:text="@string/lbl_4_5"/>
        </LinearLayout>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtMuybuenapredi"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_7pxv"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_5pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="match_parent"
            android:text="@{resenasRowModel.txtMuybuenapredi}"
            style="@style/txtOpenSansopensansmedium12_2" tools:text="@string/msg_muy_buena_predi"/>
    </LinearLayout>
</layout>
