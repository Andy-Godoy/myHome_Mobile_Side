<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="propiedadesFavoritosUsuarioOneVM" type="com.example.myhome.modules.propiedadesfavoritosusuarioone.data.viewmodel.PropiedadesFavoritosUsuarioOneVM"/>
    </data>
    <FrameLayout android:id="@+id/framePropiedadesFav"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStyle_1">
        <LinearLayout android:id="@+id/linearColumnmyhome"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginStart="@dimen/_2pxh"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowmyhome"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMyHome"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="0.33"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{propiedadesFavoritosUsuarioOneVM.propiedadesFavoritosUsuarioOneModel.txtMyHome}"
                        style="@style/txt_1" tools:text="@string/lbl_my_home"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFavoritos"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_weight="0.67"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{propiedadesFavoritosUsuarioOneVM.propiedadesFavoritosUsuarioOneModel.txtFavoritos}"
                        style="@style/txtOpenSansopensansbold14" tools:text="@string/lbl_favoritos2"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:id="@+id/linearRowsearch"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_7pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStacksearch"
                    android:layout_height="@dimen/_48pxv"
                    android:layout_width="@dimen/_290pxh" style="@style/groupStylewhite_A700_f2cornerRadius">
                    <ImageView android:id="@+id/imageSearch"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginEnd="@dimen/_14pxh"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_search"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                </FrameLayout>
                <ImageView android:id="@+id/imageFilter"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_filter"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_filter"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFiltrar"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_weight="0.15"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{propiedadesFavoritosUsuarioOneVM.propiedadesFavoritosUsuarioOneModel.txtFiltrar}"
                    style="@style/txtOpenSansopensansregular20" tools:text="@string/lbl_filtrar"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/framePropiedadFavor"
                android:layout_height="@dimen/_316pxv"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="@dimen/_388pxh" style="@style/groupStylewhite_A700_f2cornerRadius">
                <LinearLayout android:id="@+id/linearCarrouselpropi"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <FrameLayout android:id="@+id/frameStackarrowright"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_155pxv" android:layout_width="@dimen/_387pxh">
                        <com.asksira.loopingviewpager.LoopingViewPager
                            android:id="@+id/imageSliderSlider"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_155pxv"
                            android:layout_width="match_parent"
                            app:autoScroll="true" app:isInfinite="true" app:scrollInterval="5000"/>
                        <ImageView android:id="@+id/imageArrowright"
                            android:layout_gravity="bottom|end"
                            android:layout_height="@dimen/_17pxh"
                            android:layout_marginBottom="@dimen/_58pxv"
                            android:layout_marginEnd="@dimen/_6pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_58pxv"
                            android:layout_width="@dimen/_10pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowright"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
                    </FrameLayout>

                    <com.asksira.loopingviewpager.indicator.CustomShapePagerIndicator
                        android:id="@+id/indicatorPosicionesIndi"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_154pxh"
                        android:layout_marginStart="@dimen/_155pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="wrap_content"
                        app:drawable_normal="@drawable/indicator_unselected_bg_gray_400_radius_8"
                        app:drawable_selected="@drawable/indicator_selected_bg_teal_300_radius_8"
                        app:indicator_size="@dimen/_12pxh" app:indicator_spacing="@dimen/_6pxh"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtUdCounter"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_104pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_104pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{propiedadesFavoritosUsuarioOneVM.propiedadesFavoritosUsuarioOneModel.txtUdCounter}"
                    style="@style/txtOpenSansopensansbold28" tools:text="@string/lbl_u_d_210_000"/>
                <LinearLayout
                    android:id="@+id/linearColumncharcascounter"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_65pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_65pxv"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCharcasCounter"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_5pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{propiedadesFavoritosUsuarioOneVM.propiedadesFavoritosUsuarioOneModel.txtCharcasCounter}"
                        style="@style/txtOpenSansopensanssemibold20" tools:text="@string/lbl_charcas_4000"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPalermoSohoP"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{propiedadesFavoritosUsuarioOneVM.propiedadesFavoritosUsuarioOneModel.txtPalermoSohoP}"
                        style="@style/txtOpenSansopensansmedium12" tools:text="@string/msg_palermo_soho_p"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowlanguage"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_24pxv"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <FrameLayout android:id="@+id/frameStacklanguage"
                        android:layout_height="@dimen/_43pxv"
                        android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_216pxh">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLanguage"
                            android:layout_gravity="top|start"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{propiedadesFavoritosUsuarioOneVM.propiedadesFavoritosUsuarioOneModel.txtLanguage}"
                            style="@style/txtOpenSansopensanssemibold20" tools:text="@string/lbl_95_m2"/>
                        <TextView android:ellipsize="end"
                            android:gravity="top|start"
                            android:id="@+id/txtIMPECBalcnc"
                            android:layout_gravity="bottom|start"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{propiedadesFavoritosUsuarioOneVM.propiedadesFavoritosUsuarioOneModel.txtIMPECBalcnc}"
                            style="@style/txtOpenSansopensansmedium12" tools:text="@string/msg_impec_balc_n_c"/>
                    </FrameLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDormitoriosCounter"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_weight="0.38"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{propiedadesFavoritosUsuarioOneVM.propiedadesFavoritosUsuarioOneModel.txtDormitoriosCounter}"
                        style="@style/txtOpenSansopensanssemibold20" tools:text="@string/lbl_3_dormitorios"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageImageFifteen"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_50pxh"
                    android:layout_marginBottom="@dimen/_93pxv"
                    android:layout_marginEnd="@dimen/_9pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_93pxv"
                    android:layout_width="@dimen/_80pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_image15}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image15"/>
            </FrameLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" style="@style/groupStylegray_200">
            <LinearLayout android:id="@+id/linearRowhome"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearColumnhome"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageHome"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_home"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtPropiedades"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{propiedadesFavoritosUsuarioOneVM.propiedadesFavoritosUsuarioOneModel.txtPropiedades}"
                        style="@style/txtOpenSansopensansbold12" tools:text="@string/lbl_propiedades"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearNuevaPropiedad"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageFavorite"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_22pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_favorite"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_favorite"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtLanguageOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{propiedadesFavoritosUsuarioOneVM.propiedadesFavoritosUsuarioOneModel.txtLanguageOne}"
                        style="@style/txtOpenSansopensansbold12" tools:text="@string/lbl_favoritos"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearPerfil"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageUser"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_22pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtLanguageTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{propiedadesFavoritosUsuarioOneVM.propiedadesFavoritosUsuarioOneModel.txtLanguageTwo}"
                        style="@style/txtOpenSansopensansbold12" tools:text="@string/lbl_perfil"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
</layout>
