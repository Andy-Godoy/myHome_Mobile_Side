    <data>
        <variable name="nuevaPropiedadParteFourVM" type="com.example.myhome.modules.nuevapropiedadpartefour.data.viewmodel.NuevaPropiedadParteFourVM"/>
    </data>
    <FrameLayout android:id="@+id/frameNuevaPropiedad"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_4pxv"
        android:layout_width="match_parent" style="@style/groupStyle_1">
        <LinearLayout android:id="@+id/linearBackGroundNu"
            android:layout_gravity="top|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_2pxh"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowcasatwentyone"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
