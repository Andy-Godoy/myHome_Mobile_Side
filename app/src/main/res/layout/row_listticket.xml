<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listticketRowModel" type="com.example.myhome.modules.modificarpropiedadpartefour.data.model.ListticketRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearItem"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageTicket"
            android:layout_height="@dimen/_16pxh"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginTop="@dimen/_2pxv"
            android:layout_width="@dimen/_21pxh"
            android:scaleType="fitXY" android:src="@drawable/img_ticket"
            tools:ignore="ContentDescription" tools:src="@drawable/img_ticket"/>
        <TextView android:ellipsize="end" android:id="@+id/txtFoto1png"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_6pxh"
            android:layout_weight="0.45" android:layout_width="0dp"
            android:singleLine="true"
            android:text="@{listticketRowModel.txtFoto1png}"
            style="@style/txtOpenSansopensansregular20_1" tools:text="@string/lbl_foto_1_png"/>
        <TextView android:ellipsize="end" android:id="@+id/txtUploaded"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_weight="0.39" android:layout_width="0dp"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@{listticketRowModel.txtUploaded}"
            style="@style/txtIntermedium20" tools:text="@string/lbl_uploaded"/>
        <ImageView android:id="@+id/imageCheckmark"
            android:layout_height="@dimen/_10pxh"
            android:layout_marginBottom="@dimen/_2pxv"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="@dimen/_14pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_checkmark_green_500"
            tools:ignore="ContentDescription" tools:src="@drawable/img_checkmark_green_500"/>
    </LinearLayout>
</layout>
