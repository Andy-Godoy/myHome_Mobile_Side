<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="modalFeedbackVM" type="com.example.myhome.modules.modalfeedback.data.viewmodel.ModalFeedbackVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearModalFeedback"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700_f2">
        <View
            android:background="@drawable/rectangle_bg_cyan_300_radius_tl_10_tr_10"
            android:id="@+id/lineFrameThirtyFive"
            android:layout_gravity="start"
            android:layout_height="@dimen/_6pxv" android:layout_width="@dimen/_390pxh"/>
        <TextView android:gravity="start" android:id="@+id/txtLanguage"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_width="match_parent"
            android:text="@{modalFeedbackVM.modalFeedbackModel.txtLanguage}"
            style="@style/txtSourceSansProsourcesansprobold24" tools:text="@string/msg_muchas_gracias"/>
        <LinearLayout android:id="@+id/linearRowquetepareci"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:id="@+id/txtQuetepareci"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_weight="0.39" android:layout_width="0dp"
                android:text="@{modalFeedbackVM.modalFeedbackModel.txtQuetepareci}"
                style="@style/txtOpenSansopensansbold14" tools:text="@string/msg_que_te_pareci"/>
            <androidx.appcompat.widget.AppCompatRatingBar
                android:id="@+id/ratingBarRatingbar"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_37pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_weight="0.61" android:layout_width="0dp"
                android:numStars="5"
                android:progressBackgroundTint="@color/gray_500"
                android:progressTint="@color/amber_500"
                android:rating="4" style="@style/Widget.AppCompat.RatingBar"/>
        </LinearLayout>
        <TextView android:ellipsize="end" android:gravity="start"
            android:id="@+id/txtTegustarade"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_21pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{modalFeedbackVM.modalFeedbackModel.txtTegustarade}"
            style="@style/txtOpenSansopensansbold14" tools:text="@string/msg_te_gustar_a_de"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearInputexperienc"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700_f2cornerRadius">
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtMiexperiencia"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_135pxv"
                android:layout_marginEnd="@dimen/_12pxh"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{modalFeedbackVM.modalFeedbackModel.txtMiexperiencia}"
                style="@style/txtOpenSansopensansregular13_1" tools:text="@string/msg_mi_experiencia"/>
        </LinearLayout>
        <TextView android:ellipsize="end" android:id="@+id/txtAceptar"
            android:layout_gravity="end"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_15pxv"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_300pxh"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_width="wrap_content"
            android:paddingBottom="@dimen/_1pxv"
            android:paddingEnd="@dimen/_18pxh"
            android:paddingStart="@dimen/_18pxh"
            android:paddingTop="@dimen/_5pxv" android:singleLine="true"
            android:text="@{modalFeedbackVM.modalFeedbackModel.txtAceptar}"
            style="@style/txtSolidRoundedOutline_3" tools:text="@string/lbl_aceptar"/>
    </LinearLayout>
</layout>
