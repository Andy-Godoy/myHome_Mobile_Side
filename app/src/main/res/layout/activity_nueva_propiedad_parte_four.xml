                    android:layout_width="@dimen/_22pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txt4deFour"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_134pxh"
                    android:layout_weight="0.72"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{nuevaPropiedadParteFourVM.nuevaPropiedadParteFourModel.txt4deFour}"
                    style="@style/txtOpenSansopensanssemibold20" tools:text="@string/lbl_4_de_4"/>
