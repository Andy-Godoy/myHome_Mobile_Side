                            android:layout_height="@dimen/_23pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_casa21"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_casa21"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtMyHome"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_5pxh"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{nuevaPropiedadParteFourVM.nuevaPropiedadParteFourModel.txtMyHome}"
