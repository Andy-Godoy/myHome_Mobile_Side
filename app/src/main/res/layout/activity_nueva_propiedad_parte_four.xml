                        <View
                            android:background="@drawable/rectangle_bg_gray_400_radius_4"
                            android:id="@+id/viewThumb"
                            android:layout_gravity="end"
                            android:layout_height="@dimen/_70pxh"
                            android:layout_marginBottom="@dimen/_107pxv"
                            android:layout_marginEnd="@dimen/_3pxh"
                            android:layout_marginStart="@dimen/_4pxh"
                            android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_8pxh"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFotosVideos"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_31pxh"
                android:layout_marginStart="@dimen/_31pxh"
                android:layout_marginTop="@dimen/_69pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{nuevaPropiedadParteFourVM.nuevaPropiedadParteFourModel.txtFotosVideos}"
                style="@style/txtOpenSansopensansmedium14" tools:text="@string/lbl_fotos_videos"/>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRowvector"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylecornerRadius_3">
                <ImageView android:id="@+id/imageVector"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_54pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSeleccionararc"
