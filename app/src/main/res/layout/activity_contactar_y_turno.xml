<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="contactarYTurnoVM" type="com.example.myhome.modules.contactaryturno.data.viewmodel.ContactarYTurnoVM"/>
    </data>
    <FrameLayout android:id="@+id/frameContactaryTur"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStyle_1">
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" style="@style/groupStylegray_200">
            <LinearLayout android:id="@+id/linearRowhome"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearColumnhome"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageHome"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_home"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtPropiedades"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{contactarYTurnoVM.contactarYTurnoModel.txtPropiedades}"
                        style="@style/txtOpenSansopensansbold12" tools:text="@string/lbl_propiedades"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearNuevaPropiedad"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageFavorite"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_22pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_favorite"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_favorite"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtLanguage"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{contactarYTurnoVM.contactarYTurnoModel.txtLanguage}"
                        style="@style/txtOpenSansopensansbold12" tools:text="@string/lbl_favoritos"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearPerfil"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageUser"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_22pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtLanguageOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{contactarYTurnoVM.contactarYTurnoModel.txtLanguageOne}"
                        style="@style/txtOpenSansopensansbold12" tools:text="@string/lbl_perfil"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
        <LinearLayout android:id="@+id/linearBackground"
            android:layout_gravity="top|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_7pxh"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowmyhome"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMyHome"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="0.32"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{contactarYTurnoVM.contactarYTurnoModel.txtMyHome}"
                        style="@style/txt_1" tools:text="@string/lbl_my_home"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtContactar"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_weight="0.68"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{contactarYTurnoVM.contactarYTurnoModel.txtContactar}"
                        style="@style/txtOpenSansopensansbold14" tools:text="@string/lbl_contactar"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <ImageView android:id="@+id/imageImageEighteen"
                android:layout_gravity="end"
                android:layout_height="@dimen/_80pxh"
                android:layout_marginEnd="@dimen/_110pxh"
                android:layout_marginStart="@dimen/_110pxh"
                android:layout_marginTop="@dimen/_54pxv"
                android:layout_width="@dimen/_80pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_image18}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_image18"/>
            <TextView android:ellipsize="end" android:gravity="end"
                android:id="@+id/txtContactalanu"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_54pxh"
                android:layout_marginStart="@dimen/_54pxh"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{contactarYTurnoVM.contactarYTurnoModel.txtContactalanu}"
                style="@style/txtOpenSansopensansregular18" tools:text="@string/msg_contact_al_anu"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtMartinFonseca"
                android:layout_gravity="end"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_69pxh"
                android:layout_marginStart="@dimen/_69pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{contactarYTurnoVM.contactarYTurnoModel.txtMartinFonseca}"
                style="@style/txtOpenSansopensansmedium12_2" tools:text="@string/msg_martin_fonseca"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_f2_radius_8"
                android:ellipsize="end" android:hint="@string/lbl_email"
                android:id="@+id/etEmail"
                android:inputType="textEmailAddress"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_10pxh"
                android:paddingTop="@dimen/_10pxv"
                android:singleLine="true"
                android:text="@={contactarYTurnoVM.contactarYTurnoModel.etEmailValue}"
                android:textColorHint="@color/gray_500"
                style="@style/etSolidRounded" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_f2_radius_8"
                android:ellipsize="end"
                android:hint="@string/lbl_nombre"
                android:id="@+id/etNombre" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_35pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_10pxh"
                android:paddingTop="@dimen/_10pxv"
                android:singleLine="true"
                android:text="@={contactarYTurnoVM.contactarYTurnoModel.etNombreValue}"
                android:textColorHint="@color/gray_500"
                style="@style/etSolidRounded" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_f2_border_gray_400_radius_8"
                android:drawableEnd="@drawable/img_vector_gray_700"
                android:drawablePadding="@dimen/_30pxh"
                android:ellipsize="end"
                android:hint="@string/lbl_solicitar_turno"
                android:id="@+id/etDropdownconsul"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_34pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_10pxv"
                android:paddingEnd="@dimen/_15pxh"
                android:paddingStart="@dimen/_4pxh"
                android:paddingTop="@dimen/_11pxv"
                android:singleLine="true"
                android:text="@={contactarYTurnoVM.contactarYTurnoModel.etDropdownconsulValue}"
                android:textColorHint="@color/gray_500"
                style="@style/etSolidRoundedOutline_5" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end" android:id="@+id/txtFecha"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_38pxh"
                android:layout_marginStart="@dimen/_38pxh"
                android:layout_marginTop="@dimen/_36pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{contactarYTurnoVM.contactarYTurnoModel.txtFecha}"
                style="@style/txtOpenSansopensansregular18" tools:text="@string/lbl_fecha"/>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearFecha"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700_2">
                <LinearLayout android:id="@+id/linearRowcalendar"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_weight="0.91"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCalendar"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_calendar"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_calendar"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDdmmyyyy"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.84"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{contactarYTurnoVM.contactarYTurnoModel.txtDdmmyyyy}"
                        style="@style/txtOpenSansopensansregular13_1" tools:text="@string/lbl_dd_mm_yyyy"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageVector"
                    android:layout_height="@dimen/_5pxh"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_155pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_gray_500"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_500"/>
            </LinearLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtTurno"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_38pxh"
                android:layout_marginStart="@dimen/_38pxh"
                android:layout_marginTop="@dimen/_35pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{contactarYTurnoVM.contactarYTurnoModel.txtTurno}"
                style="@style/txtOpenSansopensansregular18" tools:text="@string/lbl_turno"/>
            <RadioGroup android:gravity="center_horizontal"
                android:id="@+id/rgSeleccinRadio"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <RadioButton android:buttonTint="@color/gray_500"
                    android:ellipsize="end"
                    android:fontFamily="@font/open_sans"
                    android:id="@+id/rbMaana"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingStart="@dimen/_22pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_ma_ana"
                    android:textColor="@color/gray_800"
                    android:textFontWeight="500"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="bold" tools:text="@string/lbl_ma_ana"/>
                <RadioButton android:buttonTint="@color/gray_500"
                    android:ellipsize="end"
                    android:fontFamily="@font/open_sans"
                    android:id="@+id/rbTarde"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_92pxh"
                    android:layout_width="wrap_content"
                    android:paddingStart="@dimen/_22pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_tarde"
                    android:textColor="@color/gray_800"
                    android:textFontWeight="500"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="bold" tools:text="@string/lbl_tarde"/>
            </RadioGroup>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnContactarOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_48pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_9pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_9pxv"
                android:singleLine="true"
                android:text="@string/lbl_contactar"
                android:textAllCaps="false"
                style="@style/btnSolidRoundedOutline_1" tools:text="@string/lbl_contactar"/>
        </LinearLayout>
    </FrameLayout>
</layout>
